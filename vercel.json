{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vitejs/plugin-react"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://akkanat.pythonanywhere.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
